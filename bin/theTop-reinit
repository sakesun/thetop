#!/bin/bash

export THETOP_BIN="`cd \`dirname $0\`; pwd`"
. "$THETOP_BIN/theTop-setenv"

rm "$THETOP_BIN/activate"
rm "$THETOP_HOME/local"
rm -r "$THETOP_HOME/lib"
rm -r "$THETOP_HOME/eggs"
rm -r "$THETOP_HOME/develop-eggs"
rm -r "$THETOP_HOME/include"
rm -r "$THETOP_HOME/parts"

rm "$THETOP_HOME/buildout.cfg"

"$THETOP_BIN/theTop-init"